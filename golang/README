go version: weekly.2011-12-22

goprotobuf version: go.weekly.2011-12-06

-- how to install goprotobuf --

# goinstall code.google.com/p/goprotobuf/proto does not work
# though it's written in http://code.google.com/p/goprotobuf/

export GOPATH=/home/yunabe/local/src/gocode
goinstall code.google.com/p/goprotobuf/hg/proto
cd $GOPATH/src/goprotobuf.googlecode.com/hg/compiler
hg update -r go.weekly.2011-12-06 -C
make install
